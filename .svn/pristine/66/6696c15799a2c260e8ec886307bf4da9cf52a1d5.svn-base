<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.parable.parablesdb"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="21"
        android:targetSdkVersion="21" />
    <!-- Nos da permiso para utilizar el wifi si no estaba activado -->
    <uses-permission android:name="android.permission.CHANGE_WIFI_STATE" />

    <!-- used to obtain information about the WiFi environment -->
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.parable.parablesdb.Inicio"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.parable.parablesdb.MainActivity"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.parable.parablesdb.Posicion"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.parable.parablesdb.EnviarDatos"
            android:label="@string/app_name" >
        </activity>
    </application>

</manifest>